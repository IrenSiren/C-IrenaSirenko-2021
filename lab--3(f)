//lab 3 F problem
#include <iostream>
#include <string>

using namespace std;

int* my_slightly_dumb_reallocation(int* source, int n_old, int n_new)
{
  if (source == NULL)
  {
    source = new int[n_new];
  }
  else
  {
    if (n_new != 0)
    {
      int* b = new int[n_new];

      if( n_old >= n_new)
      {
        for (int k = 0; k < n_new; k++)
        {
          b[k] = source [k];
        }
      }
      else
      {
        for (int k = 0; k < n_old; k++)
        {
          b[k] = source [k];
        }
        for (int k = n_old; k < n_new; k++)
        {
          b[k] = 0;
        }
      }
      delete source ;
      return b;

    }
    else
    {
      return NULL;
    }
  }
}

int main()
{
  int n, i;
  cin >> n;
  //int *a = my_slightly_dumb_reallocation(NULL, 0, n / 2);
  int *a = new int[n / 2];
  for (i = 0; i < n / 2; i++)
      cin >> a[i];
  a = my_slightly_dumb_reallocation(a, n / 2, n);
  for (; i < n; i++)
      cin >> a[i];
  int sum = 0;
  for (i = 0; i < n; i++)
      sum += a[i];
  cout << sum << endl;
  a = my_slightly_dumb_reallocation(a, n, n / 2);
  a = my_slightly_dumb_reallocation(a, n / 2, 0);
  a = my_slightly_dumb_reallocation(a, 0, 0);
  return 0;
}