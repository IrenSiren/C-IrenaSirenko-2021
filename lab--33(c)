//lab 3 C problem
#include <iostream>
#include <string>

using namespace std;

int main()
{
    int n;
    cout << "n= ";
    cin >> n ;
    int* numbers = new int[n];
    for (int i = 0; i < n ; i++)
    {
        cin >> numbers[i];
    }
    
    for (int i = 0; i < n ; i++)
    {
        cout << numbers[i] <<' ' ;   
    }
    
    return 0;
}

